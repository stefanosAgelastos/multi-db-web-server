// Relations between student and subject //> Needs to be executed sequentially (probably a better way to do so)
MATCH (s:Student), (f:Subject)
WHERE s.subject= 'Python' AND f.name = 'Python'
CREATE (s)-[:STUDIES]->(f)

MATCH (s:Student), (f:Subject)
WHERE s.subject= 'Java' AND f.name = 'Java'
CREATE (s)-[:STUDIES]->(f)

MATCH (s:Student), (f:Subject)
WHERE s.subject= 'Advanced Python' AND f.name = 'Advanced Python'
CREATE (s)-[:STUDIES]->(f)

MATCH (s:Student), (f:Subject)
WHERE s.subject= 'Design Patterns' AND f.name = 'Design Patterns'
CREATE (s)-[:STUDIES]->(f)

MATCH (s:Student), (f:Subject)
WHERE s.subject= 'Database' AND f.name = 'Database'
CREATE (s)-[:STUDIES]->(f)

MATCH (s:Student), (f:Subject)
WHERE s.subject= 'Ruby' AND f.name = 'Ruby'
CREATE (s)-[:STUDIES]->(f)

MATCH (s:Student), (f:Subject)
WHERE s.subject= 'C#' AND f.name = 'C#'
CREATE (s)-[:STUDIES]->(f)

MATCH (s:Student), (f:Subject)
WHERE s.subject= 'DLS' AND f.name = 'DLS'
CREATE (s)-[:STUDIES]->(f)

MATCH (s:Student), (f:Subject)
WHERE s.subject= 'Android' AND f.name = 'Android'
CREATE (s)-[:STUDIES]->(f)

MATCH (s:Student), (f:Subject)
WHERE s.subject= 'C++' AND f.name = 'C++'
CREATE (s)-[:STUDIES]->(f)

MATCH (s:Student), (f:Subject)
WHERE s.subject= 'Javascript' AND f.name = 'Javascript'
CREATE (s)-[:STUDIES]->(f)

MATCH (s:Student), (f:Subject)
WHERE s.subject= 'Testing' AND f.name = 'Testing'
CREATE (s)-[:STUDIES]->(f)

// subject node and semester node relations //
MATCH (n:Subject), (m:Semester)
WHERE m.name = 'SD22 Summer'
CREATE (n)-[:TAUGHT_IN]->(m)

MATCH (a:Subject:Winter), (b:Semester)
WHERE b.name = 'SD22 Winter'
CREATE (a)-[:TAUGHT_IN]->(b)


