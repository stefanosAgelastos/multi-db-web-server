CREATE 

// Subject nodes
(python:Subject {name:'Python'}),
(java:Subject {name:'Java'}),
(advanced_python:Subject {name:'Advanced Python'}),
(design_patterns:Subject {name:'Design Patterns'}),
(database:Subject {name:'Database'}),
(ruby:Subject {name:'Ruby'}),
(c_sharp:Subject {name:'C#'}),
(dls:Subject {name:'DLS'}),
(android:Subject {name:'Android'}),
(cplus:Subject {name:'C++'}),
(javascript:Subject {name:'Javascript'}),
(testing:Subject {name:'Testing'}),


// student nodes
(john:Student {name:'Archana', subject: 'Python', subject: 'Android', subject: 'Design Patterns', subject: 'Ruby'}),
(gabriel:Student {name:'Gabriel', subject: 'Python', subject: 'Advanced Python', subject: 'Database', subject: 'DLS'}),
(henry:Student {name:'Henry', subject: 'Database', subject: 'Android', subject: 'DLS', subject: 'Testing'}),
(ali:Student {name:'Ali', subject: 'Database', subject: 'Java', subject: 'DLS', subject: 'Ruby'}),
(archana:Student {name:'Archana', subject: 'Java', subject: 'Testing', subject: 'C#', subject: 'DLS'}),
(stefanos:Student {name:'Stefanos', subject: 'Java', subject: 'Python', subject: 'C#', subject: 'Design Patterns'}),
(muhammad:Student {name:'Muhammad', subject: 'Testing', subject: 'Advanced Python', subject: 'Database', subject: 'C#'}),
(rafael:Student {name:'Rafael', subject: 'Testing', subject: 'Ruby', subject: 'Testing', subject: 'DLS'}),
(ismail:Student {name:'Ismail', subject: 'Javascript', subject: 'Testing', subject: 'Design Patterns', subject: 'Ruby'}),


//  department node
(kea:Department {name: 'KEA Department'}),

// Semester nodes
(sd22sum: Semester {name: 'SD21 Summer'}),
(sd22win: Semester {name: 'SD21 Winter'}),

// teacher nodes
(tomas:Teacher {name: 'Tomas', email:'tomas@kea.dk'}),
(jarl:Teacher {name: 'Jarl', email:'jarl@kea.dk'}),
(asger:Teacher {name: 'Asger', email:'asbc@kea.dk'}),
(andrea:Teacher {name: 'Andrea', email:'ande@kea.dk'}),


// teacher nodes and subjects nodes relations
(tomas)-[:TEACHES]->(database),
(tomas)-[:TEACHES]->(python),
(tomas)-[:TEACHES]->(java),
(jarl)-[:TEACHES]->(advanced_python),
(jarl)-[:TEACHES]->(design_patterns),
(jarl)-[:TEACHES]->(c_sharp),
(asger)-[:TEACHES]->(dls),
(asger)-[:TEACHES]->(ruby),
(asger)-[:TEACHES]->(android),
(andrea)-[:TEACHES]->(cplus),
(andrea)-[:TEACHES]->(javascript),
(andrea)-[:TEACHES]->(testing),

// teacher nodes and department nodes
(tomas)-[:BELONGS_TO]->(kea),
(jarl)-[:BELONGS_TO]->(kea),
(asger)-[:BELONGS_TO]->(kea),
(andrea)-[:BELONGS_TO]->(kea)

//  Maybe unnecessary
// (tomas)-[:COLLEAGUE_TO]->(jarl),
// (tomas)-[:COLLEAGUE_TO]->(asger),
